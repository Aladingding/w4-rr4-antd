{
    "presets": ["@babel/preset-env","@babel/preset-react"],
    "plugins": [
        "syntax-dynamic-import",
        // 编译一些es7语法
        "@babel/plugin-proposal-class-properties",
        [
            "import", {
                "libraryName": "antd",
                "libraryDirectory": "lib", // `lib` 默认
                "style": "css" // `style: true` 会加载 less 文件
            }
        ]
    ]
}